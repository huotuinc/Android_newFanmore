<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!-- 流量环 -->
  <declare-styleable name="RingView">
        <attr name="description" format="string"></attr>
        <attr name="title" format="string"></attr>
        <attr name="titlesize" format="integer"></attr>
        <attr name="subtextsize" format="integer"></attr>
        <attr name="ringcolor" format="color"></attr>
        <attr name="percentcolor" format="color"></attr>
    </declare-styleable>
    
  <!-- 切换开关 -->
  <declare-styleable name="slideswitch">
        <attr name="themeColor" format="color" />
        <attr name="isOpen" format="boolean" />
        <attr name="shape">
            <enum name="rect" value="1" />
            <enum name="circle" value="2" />
        </attr>
    </declare-styleable>
</resources>
